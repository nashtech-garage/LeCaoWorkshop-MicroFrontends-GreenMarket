{
  "name": "@app/root-config",
  "version": "0.0.8",
  "scripts": {
    "start": "concurrently --kill-others --kill-others-on-fail -p name --names \"common-app,contact-form-app,common-header-app,common-hero-search-app,common-footer-app,main-featured-product-app,react-main-page-hero-banner-app,mainpage-blog-app,root-config\" \"npm run start:common-app\" \"npm run start:angular-apps:contact-form-app\" \"npm run start:angular-apps:common-header-app\" \"npm run start:react-apps:common-hero-search-app\" \"npm run start:react-apps:common-footer-app\" \"npm run start:react-apps:main-featured-product-app\" \"npm run start:react-apps:react-main-page-hero-banner-app\" \"npm run start:vue-apps:mainpage-blog-app\" \"npm run start:vue-apps:detailpage-blog-app\" \"webpack serve --port 9000 --env isLocal\"",
    "start:common-app": "cd ../common-app/ && npx http-server . --cors -p 5000",
    "start:angular-apps:contact-form-app": "cd ../angular-apps/contact-form-app/dist/contact-form-app && npx http-server . --cors -p 6003",
    "start:angular-apps:common-header-app": "cd ../angular-apps/common-header-app/dist/common-header-app && npx http-server . --cors -p 5001",
    "start:react-apps:common-hero-search-app": "cd ../react-apps/common-hero-search-app/dist && npx http-server . --cors -p 5003",
    "start:react-apps:common-footer-app": "cd ../react-apps/common-footer-app/dist && npx http-server . --cors -p 5005",
    "start:react-apps:main-featured-product-app": "cd ../react-apps/main-featured-product-app/dist && npx http-server . --cors -p 9003",
    "start:react-apps:react-main-page-hero-banner-app": "cd ../react-apps/react-main-page-hero-banner-app/dist && npx http-server . --cors -p 9001",
    "start:vue-apps:mainpage-blog-app": "cd ../vue-apps/mainpage-blog-app/dist && http-server . --cors -p 9004",
    "start:vue-apps:detailpage-blog-app": "cd ../vue-apps/detailpage-blog-app/dist && http-server . --cors -p 9006",
    "lint": "eslint src --ext js,ts,tsx",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "install-all": "npm run install:root-config && npm run install:angular-apps:contact-form-app && npm run install:angular-apps:common-header-app && npm run install:react-apps:common-hero-search-app && npm run install:react-apps:main-featured-product-app && npm run install:react-apps:common-footer-app && npm run install:react-apps:react-main-page-hero-banner-app && npm run install:vue-apps:mainpage-blog-app && npm run install:vue-apps:detailpage-blog-app",
    "install:root-config": "npm i",
    "install:angular-apps:contact-form-app": "cd ../angular-apps/contact-form-app/ && npm i",
    "install:angular-apps:common-header-app": "cd ../angular-apps/common-header-app/ && npm i",
    "install:react-apps:common-hero-search-app": "cd ../react-apps/common-hero-search-app/ && npm i",
    "install:react-apps:common-footer-app": "cd ../react-apps/common-footer-app/ && npm i",
    "install:react-apps:main-featured-product-app": "cd ../react-apps/main-featured-product-app/ && npm i",
    "install:react-apps:react-main-page-hero-banner-app": "cd ../react-apps/react-main-page-hero-banner-app/ && npm i",
    "install:vue-apps:mainpage-blog-app": "cd ../vue-apps/mainpage-blog-app/ && npm i",
    "install:vue-apps:detailpage-blog-app": "cd ../vue-apps/detailpage-blog-app/ && npm i",
    "build": "npm run build:angular-apps:contact-form-app && npm run build:angular-apps:common-header-app && npm run build:react-apps:common-hero-search-app && npm run build:react-apps:common-footer-app && npm run build:react-apps:main-featured-product-app && npm run build:react-apps:react-main-page-hero-banner-app && npm run build:vue-apps:mainpage-blog-app && npm run build:vue-apps:detailpage-blog-app",
    "build:angular-apps:contact-form-app": "cd ../angular-apps/contact-form-app/ && npm run build:single-spa:contact-form-app",
    "build:angular-apps:common-header-app": "cd ../angular-apps/common-header-app/ && npm run build:single-spa:common-header-app",
    "build:react-apps:common-hero-search-app": "cd ../react-apps/common-hero-search-app/ && copy .env.example .env && webpack --config ./webpack.config.js",
    "build:react-apps:common-footer-app": "cd ../react-apps/common-footer-app/ && webpack --config ./webpack.config.js",
    "build:react-apps:main-featured-product-app": "cd ../react-apps/main-featured-product-app/ && copy .env.example .env && webpack --config ./webpack.config.js",
    "build:react-apps:react-main-page-hero-banner-app": "cd ../react-apps/react-main-page-hero-banner-app/ && copy .env.example .env && webpack --config ./webpack.config.js",
    "build:vue-apps:mainpage-blog-app": "cd ../vue-apps/mainpage-blog-app/ && npm run build",
    "build:vue-apps:detailpage-blog-app": "cd ../vue-apps/detailpage-blog-app/ && npm run build",
    "build:webpack": "webpack --mode=production",
    "build:types": "tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-ts-important-stuff": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "serve": "^14.2.0",
    "ts-config-single-spa": "^3.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.8.0",
    "webpack-config-single-spa-ts": "^4.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@types/jest": "^27.0.1",
    "@types/systemjs": "^6.1.1",
    "@types/webpack-env": "^1.16.2",
    "bootstrap": "^4.4.1",
    "global": "^4.4.0",
    "http-server": "^14.1.1",
    "single-spa": "^5.9.3",
    "single-spa-layout": "^1.6.0"
  },
  "types": "dist/app-root-config.d.ts"
}
